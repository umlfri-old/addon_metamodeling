<Domain id="link" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" >
    <Attribute id="withMe" name="Identity relationship" >
		<Bool default="False"/>
	</Attribute>
    <Attribute id="allowRecursive" name="Recursive" >
		<Bool default="False"/>
	</Attribute>
    <Attribute id="icon" name="Icon" hidden="true">
		<Str default="icons/link.png"></Str>
	</Attribute>
    <Attribute id="circleIcon" name="Circle Icon" hidden="true">
		<Str default="icons/circle.png"></Str>
	</Attribute>
    <Attribute id="note" name="Note" >
		<Text/>
	</Attribute>
</Domain>
