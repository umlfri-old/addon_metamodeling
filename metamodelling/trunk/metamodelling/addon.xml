<?xml version="1.0" ?>
<AddOn xmlns="http://umlfri.org/xmlschema/addon.xsd">
    <Identity uri="urn:umlfri.org:plugin:metamodellingComposite" />
    
    <FriendlyName name="Metamodelling" version="0.1.1"/>
    <Author>
        <Name name="Michal Kováčik" />
        <Homepage url="http://umlfri.org" /> 
        <CommonLicense name="GPL-2" />
    </Author>
    
    <Icon path="icons/icon.png" />
    <Description>
        Metamodelling plug-in is a tool for creating your own diagrams and extending the editor.
    </Description>
    
    <Composite>
        <Metamodel>
            <Path path="metamodel" />
            <Template path="templates/empty.fritx" name="Empty Graph" />
        </Metamodel>
        
		<Plugin>
			<Code os="all" language="python" path="plugin" />
		</Plugin>
    </Composite>
    
</AddOn>
