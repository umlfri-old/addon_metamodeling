<ElementType id="Object" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd">
    <Icon path="icons/Object.png"/>
    <Domain id="Object"/>
    <Connections>
        <Item value="Relationship"/>
    </Connections>
    <Appearance>
        <Shadow padding="3" color="#cfg.Styles.Element.ShadowColor">
            <Rectangle fill="#cfg.Styles.Element.FillColor" border="#cfg.Styles.Element.LineColor" right="10 rounded_side" left="10 rounded_side">
                <Sizer minheight="50" minwidth="100">
                    <Padding padding="5"> 
						<Condition condition="#self.object_name">			
							<Align align="center center"> 
								<VBox expand="0">
									<TextBox text="&lt;&lt;" font="#cfg.Styles.Connection.NameTextFont" color="#cfg.Styles.Connection.NameTextColor"/>
									<TextBox text="#self.object_name" font="#cfg.Styles.Connection.NameTextFont" color="#cfg.Styles.Connection.NameTextColor"/>
									<TextBox text="&gt;&gt;" font="#cfg.Styles.Connection.NameTextFont" color="#cfg.Styles.Connection.NameTextColor"/>
								</VBox>
							</Align> 
						</Condition>	        
                    </Padding>    
                </Sizer>
            </Rectangle>
        </Shadow>
    </Appearance>
    <Options>
    </Options>
</ElementType>